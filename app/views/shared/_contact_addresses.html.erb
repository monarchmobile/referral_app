<ul class="address_info">
	<% if contact.addresses.count > 0 %>
	  <% contact.addresses.each do |a| %>
	    <%= content_tag :li, a.address1 unless a.address1 == "" %>
	    <%= content_tag :li, a.address2 unless a.address2 == "" %>
	    <%= content_tag :li, "#{a.city}, #{a.state}" %>
	    <%= content_tag :li, a.country unless a.country == "" %>
	  <% end %>
	<% end %>
</ul>
